#
# BUG#21454472 AUTO-INCREMENT SEQUENCE GETS RESET
#

--source include/have_innodb.inc
--source include/have_debug.inc

CREATE TABLE t1 (
  id    INT AUTO_INCREMENT PRIMARY KEY,
  name  VARCHAR(100)
) ENGINE=InnoDB AUTO_INCREMENT=99;

set @save_debug_dbug= @@debug_dbug;
SET @@debug_dbug="+d,innodb_evict_autoinc_table";

# Evict t1 from dictionary cache
-- error ER_WRONG_VALUE_FOR_VAR
SET GLOBAL innodb_ft_aux_table="test/t1";

SET @@debug_dbug="-d,innodb_evict_autoinc_table";

INSERT INTO t1(name) VALUES('mysql');

SELECT * FROM t1;

DROP TABLE t1;
set @@debug_dbug= @save_debug_dbug;